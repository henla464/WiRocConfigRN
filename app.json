{
  "expo": {
    "name": "WiRocConfigRN",
    "slug": "WiRocConfigRN",
    "version": "v19",
    "icon": "./images/icon-512x512.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./images/icon-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wirocconfigrn"
    },
    "android": {
      "enableHermes": true,
      "adaptiveIcon": {
        "foregroundImage": "./images/icon-512x512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wirocconfigrn",
      "versionCode": 19
    },
    "plugins": [
      "react-native-ble-plx",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24
          }
        }
      ]
    ]
  }
}
